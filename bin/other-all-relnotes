#!/bin/bash

this="${BASH_SOURCE-$0}"
bin=$(cd -P -- "$(dirname -- "${this}")" >/dev/null && pwd -P)

OPATH=${PATH}
PATH=${bin}:${OPATH}

cd ${HOME}/Src/aw-github/hadoop-release-metadata/HBASE
projdocmaker.py --project HBASE --version 1.1.1
projdocmaker.py --project HBASE --version 1.1.0
projdocmaker.py --project HBASE --version 1.0.2
projdocmaker.py --project HBASE --version 1.0.1
projdocmaker.py --project HBASE --version 1.0.0

cd ${HOME}/Src/aw-github/hadoop-release-metadata/HIVE
projdocmaker.py --project HIVE --version 1.2.0
projdocmaker.py --project HIVE --version 1.1.1
projdocmaker.py --project HIVE --version 1.1.0
projdocmaker.py --project HIVE --version 1.0.1
projdocmaker.py --project HIVE --version 1.0.0
projdocmaker.py --project HIVE --version 0.15.0
projdocmaker.py --project HIVE --version 0.14.1
projdocmaker.py --project HIVE --version 0.14.0
projdocmaker.py --project HIVE --version 0.13.1
projdocmaker.py --project HIVE --version 0.13.0








d=$(date -u "+%Y-%m-%d %H:%m")
git commit -a -m "${d} UTC auto-commit"

PATH=${OPATH}
