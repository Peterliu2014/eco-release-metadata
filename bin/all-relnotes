#!/bin/bash

this="${BASH_SOURCE-$0}"
bin=$(cd -P -- "$(dirname -- "${this}")" >/dev/null && pwd -P)

OPATH=${PATH}
PATH=${bin}:${OPATH}

cd ${HOME}/Src/aw-github/hadoop-release-metadata

relnotes 3.0.0
relnotes 2.8.0
relnotes 2.7.0
relnotes 2.6.1
relnotes 2.6.0
relnotes 2.5.2
relnotes 2.5.1
relnotes 2.5.0
relnotes 2.4.1
relnotes 2.4.0
relnotes 2.3.0
relnotes 2.2.1
relnotes 2.0.0-alpha

d=$(date -u "+%Y-%m-%d %H:%m")
git commit -m "${d} UTC auto-commit"

PATH=${OPATH}
